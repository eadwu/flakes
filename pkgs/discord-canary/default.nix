    pname = "discord";
    version = "0.0.286";

    src = fetchurl {
      url = "https://dl-canary.discordapp.net/apps/linux/${version}/discord-canary-${version}.tar.gz";
      sha256 = "1pyax32ap1nqqm4a388wl953zihsd8zlilxxjsam85ii2j4ds744";
    };
  }
)
